      "import": "./dist/styles/index.css"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch"
  },
  "dependencies": {
    "vue": "^3.4.0",
    "mdui": "^2.2.0",
    "@mdui/icons": "^2.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0"
  },
  "peerDependencies": {
    "vue": "^3.4.0",
    "mdui": "^2.2.0"
  }
}
